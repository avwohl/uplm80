/*
 * SIMPLE TEST - JUST PRINT 3
 */

0100H:

TESTSUITE:
DO;

MON1: PROCEDURE(FUNC, PARM) EXTERNAL;
    DECLARE FUNC BYTE, PARM ADDRESS;
END MON1;

/* PRINT SINGLE CHARACTER */
PRINT$CHAR: PROCEDURE(C);
    DECLARE C BYTE;
    CALL MON1(2, C);
END PRINT$CHAR;

/* SIMPLE MAIN */
MAIN: PROCEDURE;
    CALL PRINT$CHAR('3');
    CALL PRINT$CHAR(13);
    CALL PRINT$CHAR(10);
END MAIN;

CALL MAIN;

END TESTSUITE;

EOF
